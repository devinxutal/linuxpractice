<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.devinxutal.man20" android:versionCode="4"
	android:versionName="1.1.0">
	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:theme="@style/CustomButton">
		<activity android:name="com.devinxutal.man20.activities.MainActivity"
			android:label="@string/app_name" android:screenOrientation="landscape">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="com.devinxutal.man20.activities.PlaygroundActivity"
			android:label="@string/app_name" android:screenOrientation="landscape">
		</activity>
		<activity android:name="com.devinxutal.man20.activities.Preferences"
			android:label="@string/preferences_title" android:screenOrientation="landscape">
		</activity>
		<activity android:name="com.devinxutal.man20.activities.AboutActivity"
			android:label="@string/common_about" android:screenOrientation="landscape">
		</activity>

		<!-- The application's publisher ID assigned by AdMob -->
		<meta-data android:value="a14d5bb6038850d" android:name="ADMOB_PUBLISHER_ID" />
		<!-- AdMobActivity definition -->
		<activity android:name="com.admob.android.ads.AdMobActivity"
			android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
			android:configChanges="orientation|keyboard|keyboardHidden" />
		<!-- Track Market installs -->
		<receiver android:name="com.admob.android.ads.analytics.InstallReceiver"
			android:exported="true">
			<intent-filter>
				<action android:name="com.android.vending.INSTALL_REFERRER" />
			</intent-filter>
		</receiver>
		<meta-data android:value="true" android:name="ADMOB_ALLOW_LOCATION_FOR_ADS" />
	</application>
	<uses-sdk android:minSdkVersion="4" />
	<uses-permission android:name="android.permission.VIBRATE" />
	<uses-permission android:name="android.permission.WAKE_LOCK" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


</manifest> 